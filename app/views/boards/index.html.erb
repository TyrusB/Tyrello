<div id="content"></div>


<form action="<%= session_url %>" method="POST">
	<input
		type="hidden"
		name="_method"
		value="DELETE"
	>
	<input
		type="hidden"
		name="authenticity_token"
		value="<%= form_authenticity_token %>"
	>
	<input type="submit" value="Log Out">
</form>

<script type="text/javascript">

$(function () {
	Trellino.initialize();
	Trellino.authToken = "<%= form_authenticity_token %>";
})
</script>

<script type="application/json" id="bootstrapped_boards_json">
<%= @current_user.boards.to_json.html_safe %>
</script>