<div id="content"></div>

<%#= button_to "Log Out", "#{session_url}", method: "DELETE"%>

<form action="<%= session_url %>" method="POST">
	<input
		type="hidden"
		name="_method"
		value="DELETE"
	>
	<input
		type="hidden"
		name="authenticity_token"
		value="<%= form_authenticity_token %>"
	>
	<input type="submit" value="Log Out">
</form>