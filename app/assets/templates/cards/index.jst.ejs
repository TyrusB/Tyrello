<ul class="card_list" data-list_id="<%= list.id %>">

  <li class="placeholder">
    No Cards
  </li>

  <% cards.each(function (card) { %>
    <li
      class="card_entry"
      id="card_<%= card.id %>"
      data-id="<%= card.id %>"
      data-list_id="<%= card.escape('list_id') %>"
    >
      <span
        class="card_entry"
        data-id="<%= card.id %>"
        data-list_id="<%= card.escape('list_id') %>"
      >
    		<%= card.escape('title') %>
      </span>
  		<span class="deleteCard icon-trash hidden" data-id="<%= card.id %>" ></span>
    </li>
  <% }); %>
</ul>
<div class="add_button">
<button class="addCard">Add a Card</button>
</div>