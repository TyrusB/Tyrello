<h1><%= board.escape('title') %></h1>

<ul class="list_list">
<% lists.each(function (list) { %>
  <li class="list_entry" data-id="<%= list.id %>" id="list_<%= list.id %>">
    <span class="list_entry" data-id="<%= list.id %>">
      <%= list.escape('title') %>
    </span>
  </li>
<% }) %>
</ul>

<div class="add_button">
<button class="newList btn">Add a List</button>
</div>

<button class="deleteBoard btn">Delete this Board</button>